{
  "name": "eth-txn-bundler",
  "version": "0.1.0",
  "description": "Pulls in transaction details for each event and bundles events by transaction hash ",
  "author": "mdcoon <mdcoon1@yahoo.com>",
  "homepage": "https://github.com/buidlhub/log-event-router#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buidlhub/log-event-router.git"
  },
  "scripts": {
    "test": "DEBUG=TxnBundler:* mocha --require babel-core/register --require babel-polyfill $(find ./src -name *.test.js)",
    "prepare": "babel ./src --out-dir ./dist --ignore '**/*.test.js' --source-maps --copy-files",
    "build": "npm run prepare"
  },
  "bugs": {
    "url": "https://github.com/buidlhub/log-event-router/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.21.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.1.0",
    "mocha": "^6.1.4"
  },
  "dependencies": {
    "eth-data": "^0.1.0",
    "stream-logger": "^0.1.0"
  }
}
